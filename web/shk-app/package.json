{
  "name": "shk-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --live-reload 0 --watch --port 4200 --output-path \"./dist_dev\"",
    "build:dev": "ng build --base-href \"/\" --deploy-url \"/shk-app/bundle-dev/\" --output-path \"./bundle-dev\" --watch",
    "build:prod": "ng build --prod --env=prod --base-href \"/\" --deploy-url \"/shk-app/bundle/\" --output-path \"./bundle\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "ng-xi18n",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --sort -m _ --format namespaced-json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.11",
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/platform-server": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@types/jquery": "^2.0.49",
    "@types/lodash": "^4.14.112",
    "bootstrap": "^4.1.2",
    "core-js": "^2.5.7",
    "jquery": "^3.3.1",
    "ng2-dragula": "^2.0.2",
    "nouislider": "^11.1.0",
    "npm": "^6.0.1",
    "popper.js": "^1.14.3",
    "primeng": "^5.2.7",
    "quill": "^1.3.6",
    "rxjs": "^5.5.11",
    "wnumb": "^1.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/language-service": "^5.2.11",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.114",
    "@types/yargs": "^8.0.2",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.6.2"
  }
}
